{
  "name": "puppet-puppetserver",
  "version": "2.1.0-rc0",
  "author": "Vox Pupuli",
  "summary": "Puppet module for puppetserver",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-puppetserver",
  "project_page": "https://github.com/voxpupuli/puppet-puppetserver",
